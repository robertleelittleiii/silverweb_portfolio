
<div id="image_<%= image.id%>" data-id='image_<%=image.id.to_s%>' class="imageSingle">
  <div class="imageThumb">
    <%= image_tag(image.image_url(:large).to_s) %>
  </div>
  <div class="imageName">
    <%@image=image%>
    Location     <%#=editablefieldcreate("title", image)%> <br>
     <%=ajax_select2("title","image",@image,(@image_locations + ["n/a"]+[image.title]).uniq)%> <br>
    <%=editablecheckboxedit("active_flag", @image, "Visible")%>
  </div>
   <div class="imageDelete">
   <%=link_to(image_tag("interface/Button-Delete.png"), {:id=>image, :incoming_id=>params[:id], :action=>"delete_image", :format=>"js"}, :class=>"delete_image", :confirm => 'Are you sure?', :remote=>true) %> 
   
   </div>
</div>
